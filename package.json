{
  "name": "ridge-moore-exorcism",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "docker:build": "docker build -t ridge-moore-exorcism .",
    "docker:run": "docker run --rm -p 80:8080 ridge-moore-exorcism",
    "docker:start": "npm run docker:build && npm run docker:run",
    "docker:dev": "docker-compose up --build",
    "reinstall": "rmdir /s /q node_modules && del package-lock.json && npm install"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.4",
    "@angular/compiler": "^20.1.4",
    "@angular/core": "^20.1.4",
    "@angular/forms": "^20.1.4",
    "@angular/platform-browser": "^20.1.4",
    "@angular/router": "^20.1.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.4",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "^20.1.4",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.6.0",
    "typescript": "~5.8.2"
  }
}

